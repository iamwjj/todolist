{
	"name": "Todo list",
	"version": "1.0.0",
	"manifest_version": 2,

	"description": "Vue TODO List",
	"icons":{
		"128": "img/icon.png"
	},

	"browser_action":{
    	"default_icon": "img/icon.png",
    	"default_popup": "index.html"
  	},
  	//使用Vue开发Chrome插件的时候需要加上下面这行
  	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}